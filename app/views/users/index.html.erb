<body>
  <div class="card">

  <h3 id="greeting"><%= "Here's your Patient Appointment History, #{current_user.name.upcase}" %></h3>
  </div>
  <!-- <div class="container"> -->
  <div class="row">

    <div class="col s12 m4 l3" id="left">
      <div class="date">
          <div class="card">
            <%= DateTime.now.strftime("%m/%d/%Y %H:%M") %>
          </div>
      </div>
      <div class="appointment">
        <div class="card">
          <div class="link"><%= link_to "Make an Appointment", new_appointment_path %></div>
        </div>
      </div>
      <div class="appointmentqueue">
        <div class="card">
          <div class="link"><%= link_to "Chat With Your Doctor", appointment_path(current_user) %></div>
        </div>
      </div>
      <div class="history">
        <div class="card">
          <div class="link"><%= link_to "Patient History", users_path %></div>
        </div>
      </div>
      <div class="edit">
        <div class="card">
          <div class="link">  <%= link_to "Edit Profile", edit_user_path(current_user) %>
          </div>
        </div>
      </div>
      <div class="logout">
        <div class="card">
          <div class="link"><%= link_to "Logout", root_path, method: :delete %></div>
        </div>
      </div>
      </div>

    <div class="col s12 m8 l9" id="right">
        <table>
          <thead>
            <tr>
              <th>Date Visited</th>
              <th>Symptoms</th>
              <th>Follow Up Notes</th>
            </tr>
              <!-- <iframe src="https://tokbox.com/embed/embed/ot-embed.js?embedId=51e6171d-9d47-4c82-ae87-35a53a8c3a68&room=DEFAULT_ROOM&iframe=true" width="600px" height="500px" allow="microphone; camera" ></iframe> -->
          </thead>
          <tbody>
              <% @appointments.sort_by(&:created_at).reverse.each do |a| %>
              <%# unless doctor_id == nil %>
            <tr>
              <td><%= a.created_at.strftime("%m/%d/%Y") %></td>
              <td><%= a.symptoms %></td>
              <td><%= a.follow_up %>
            <% end %>
        </tr>
</tbody>
</thead>
</table>
</div>
</div>
