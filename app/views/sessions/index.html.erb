<body>
  <div class="card">

  <h3 id="greeting"><%= "Welcome back, #{current_user.name.upcase}!" %></h3>
  </div>
  <!-- <div class="container"> -->
  <div class="row">

    <div class="col s12 m4 l3" id="left">
      <div class="date">
          <div class="card">
            <%= DateTime.now.strftime("%m/%d/%Y %H:%M") %>
          </div>
      </div>
      <div class="appointment">
        <div class="card">
          <div class="link"><%= link_to "Make an Appointment", new_charge_path %></div>
        </div>
      </div>
      <!-- session_path(current_user) -->
      <div class="history">
        <div class="card">
          <div class="link"><%= link_to "Patient History", users_path %></div>
        </div>
      </div>
      <div class="edit">
        <div class="card">
          <div class="link">  <%= link_to "Edit Profile", edit_user_path(current_user) %>
          </div>
        </div>
      </div>
      <div class="logout">
        <div class="card">
          <div class="link"><%= link_to "Logout", root_path, method: :delete %></div>
        </div>
      </div>
      </div>

      <div class="col s12 m8 l9" id="right">

      <p id="last">Your last sign-in was <%= Date.yesterday.strftime("%m/%d/%Y") %></p>
      <p id="message"><i class="material-icons">email</i>Message from your last Virtual Visit</p>
      <p id="view"><i class="material-icons">ondemand_video</i>Watch your last Virtual Visit Video</p>
    <div class="script">
      <img class="responsive-img" src="logo-red-flag-tm.png" alt="walgreens logo">
      <p id="pickup">Your Medication has been called into the pharmacy of your choice</p>
    </div>
      <!-- <div class="video-container">
      <iframe src="https://tokbox.com/embed/embed/ot-embed.js?embedId=51e6171d-9d47-4c82-ae87-35a53a8c3a68&room=DEFAULT_ROOM&iframe=true" width="600px" height="500px" allow="microphone; camera" ></iframe></div>
    </div> -->
  </div>
</div>
<!-- </div> -->
</body>


<br>
